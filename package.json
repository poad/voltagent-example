    "@voltagent/logger": "^2.0.2",
    "@voltagent/server-hono": "^2.0.4",
    "ai": "^6.0.44",
    "dotenv": "^17.2.3",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/node": "^22.19.7",
    "tsdown": "^0.15.12",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=20.19.0"
  },
  "packageManager": "pnpm@10.28.1"
